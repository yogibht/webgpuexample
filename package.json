{
    "name": "webgputest",
    "version": "1.0.0",
    "description": "Simple WebGPU triangle render test",
    "private": true,
    "scripts": {
        "cleanup": "rimraf dist/!(*.gitkeep)",
        "compile": "tsc --project tsconfig.json",
        "build": "npm run cleanup && webpack --config webpack.config.js --stats minimal",
        "start": "npm run cleanup && ts-node --project tsconfig.json server/server.ts",
        "lint:check": "eslint src --ext .ts",
        "lint:fix": "eslint src --fix --ext .ts"
    },
    "author": "YOGI",
    "license": "ISC",
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.0",
        "@types/express": "^4.17.14",
        "@types/node": "^18.11.9",
        "@types/webpack-env": "^1.18.0",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "@webgpu/types": "^0.1.30",
        "babel-loader": "^9.1.2",
        "css-loader": "^6.7.2",
        "eslint": "^8.28.0",
        "express": "^4.18.2",
        "file-loader": "^6.2.0",
        "lit": "^2.4.1",
        "redux-thunk": "^2.4.2",
        "rimraf": "^3.0.2",
        "sass": "^1.56.1",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.0",
        "webpack-dev-middleware": "^6.0.0"
    }
}
